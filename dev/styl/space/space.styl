$margin = 1.5em

.Wrapper
  display flex
  height 100%
  width 100%
  position relative

  .Space
    margin-left 10%
    margin-right 10%
    width 80%
    display block
    position relative
    .collapsible
      width 100%
    .send-btn
      position fixed
      bottom 20px
      right 20px

    .save-btn
      position fixed
      bottom 20px
      right 190px

    .export-btn
      position fixed
      bottom 20px
      right 320px

    &-inner
      margin-right1em
      width 65%

    &-content
      padding 0 !important
      background white
      padding-right $margin / 2

    &-right
      padding 0 !important
      width 35%

@import "formulation"
@import "variables"
@import "expresions"
@import "fileSideBar"